{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "parser": "babel-eslint",
  "plugins": ["babel", "prettier"],
  "env": {
    "browser": true,
    "process": true
  },
  "globals": {
    "window": true,
    "document": true
  },
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "react/prop-types": 0,
    "no-extraneous-dependencies": 0,
    "no-unresolved": 0,
    "no-console": 0,
    "max-len": 0
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    },
    "import/parser": "babel-eslint",
    "ecmaFeatures": {
      "classes": true,
      "jsx": true
    }
  }
}
